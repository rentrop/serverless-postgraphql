{
  "name": "atxfloodsadmin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addon-storyshots": "^3.1.9",
    "@storybook/react": "^3.1.9",
    "amazon-cognito-identity-js": "^1.16.0",
    "apollo-client": "^1.6.0",
    "aws-sdk": "^2.49.0",
    "get-graphql-schema": "^2.1.0",
    "jwt-decode": "^2.2.0",
    "mapbox-gl": "^0.39.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^16.0.0-rc.3",
    "react-apollo": "^1.4.2",
    "react-dom": "^16.0.0-rc.3",
    "react-mapbox-gl": "^2.2.0",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.0.0-rc.3",
    "storybook-addon-apollo-graphql": "^0.1.2",
    "tachyons": "^4.7.4"
  },
  "devDependencies": {
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "start-local": "REACT_APP_AWS_ENDPOINT='http://localhost:5000/graphql' yarn run start",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "get-schema": "./stories/schema/getSchema.sh",
    "build": "react-scripts build",
    "test": "yarn build-css; react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook"
  }
}
