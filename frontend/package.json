{
  "name": "atxfloodsadmin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^9.0.1",
    "@mapbox/spritezero-cli": "^2.1.0",
    "@storybook/addon-storyshots": "^3.1.9",
    "@storybook/cli": "^3.2.9",
    "@storybook/react": "^3.1.9",
    "amazon-cognito-identity-js": "^1.16.0",
    "apollo-client": "^1.9.2",
    "aws-sdk": "^2.117.0",
    "classnames": "^2.2.5",
    "get-graphql-schema": "^2.1.0",
    "jwt-decode": "^2.2.0",
    "mapbox-gl": "^0.40.0",
    "moment": "^2.18.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^16.0.0-rc.3",
    "react-apollo": "^1.4.2",
    "react-container-query": "^0.9.1",
    "react-dom": "^16.0.0-rc.3",
    "react-fontawesome": "^1.6.1",
    "react-mapbox-gl": "^2.5.0",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^16.0.0-rc.3",
    "storybook-addon-apollo-graphql": "^0.1.2",
    "tachyons": "^4.7.4"
  },
  "devDependencies": {
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "start-local": "REACT_APP_AWS_ENDPOINT='http://localhost:5000/graphql' yarn run start",
    "validate-map-style": "node src/Map/style/scripts/validate.js",
    "build-map-style": "yarn build-map-icons; node src/Map/style/scripts/build.js",
    "build-map-icons": "spritezero public/mapboxstyle/sprite src/Map/style/icons/; spritezero --retina public/mapboxstyle/sprite@2x src/Map/style/icons/",
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "get-schema": "./src/stories/schema/getSchema.sh",
    "build": "react-scripts build",
    "test": "yarn build-css; react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9001 -c .storybook & yarn watch-css"
  }
}
